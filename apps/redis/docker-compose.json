{
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json",
  "services": [
    {
      "name": "redis",
      "image": "redis:8.0.2-alpine",
      "isMain": true,
      "internalPort": 6379,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/redis.conf",
          "containerPath": "/usr/local/etc/redis/redis.conf"
        }
      ]
    }
  ]
}
